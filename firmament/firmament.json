[
  {
    "name": "data-container",
    "Image": "lukewendling/data-container:1",
    "DockerFilePath": "",
    "Hostname": "data-container"
  },
  {
    "name": "elasticsearch",
    "Image": "elasticsearch:2.1",
    "DockerFilePath": "",
    "Hostname": "elasticsearch",
    "ExposedPorts": {
      "9200/tcp": {}
    },
    "Env": [
      "ES_HEAP_SIZE=4g"
    ],
    "HostConfig": {
      "VolumesFrom": [
        "data-container"
      ],
      "PortBindings": {
        "9200/tcp": [
          {
            "HostPort": "9200"
          }
        ]
      }
    }
  },
  {
    "name": "redis",
    "Image": "redis:3.0",
    "DockerFilePath": "",
    "Hostname": "redis",
    "ExposedPorts": {
      "6379/tcp": {}
    },
    "HostConfig": {
      "PortBindings": {
        "6379/tcp": [
          {
            "HostPort": "6379"
          }
        ]
      }
    }
  },
  {
    "name": "caffe",
    "Image": "lukewendling/caffedata:1",
    "DockerFilePath": "",
    "Hostname": "caffe",
    "ExposedPorts": {
      "8080/tcp": {}
    },
    "HostConfig": {
      "Binds": [
      ],
      "Links": [
        "redis:redis",
        "elasticsearch:elasticsearch"
      ],
      "PortBindings": {
        "8080/tcp": [
          {
            "HostPort": "80"
          }
        ],
        "8701/tcp": [
          {
            "HostPort": "8701"
          }
        ]
      }
    },
    "ExpressApps": [
      {
        "GitUrl": "https://github.com/Sotera/social-sandbox.git",
        "GitSrcBranchName": "lbconversion",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8701",
        "ServiceName": "social-sandbox",
        "DoBowerInstall": true,
        "DeployExisting": true,
        "ClusterSize": 1,
        "EnvironmentVariables": {
          "PORT": 8080
        },
        "Scripts": [
          {
            "RelativeWorkingDir": ".",
            "Command": "cp",
            "Args": [
              "server/config.js.template",
              "server/config.js"
            ]
          }
        ]
      }
    ]
  }

]
